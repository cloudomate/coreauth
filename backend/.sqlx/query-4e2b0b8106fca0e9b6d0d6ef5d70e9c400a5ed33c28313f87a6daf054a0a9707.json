{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                id,\n                name,\n                type as connection_type,\n                scope as \"scope: ConnectionScope\",\n                organization_id,\n                config,\n                is_enabled,\n                created_at,\n                updated_at\n            FROM connections\n            WHERE organization_id = $1\n              AND scope = 'organization'\n              AND type IN ('oidc', 'saml', 'oauth2')\n              AND is_enabled = true\n            ORDER BY created_at ASC\n            LIMIT 1\n            ",
